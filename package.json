{
  "name": "order-service",
  "version": "1.0.0",
  "description": "Order service handling orders with hexagonal architecture",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/start.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write",
    "generate:swagger": "tsoa swagger",
    "generate:routes": "tsoa routes",
    "docs": "tsoa spec-and-routes",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "new-migration": "docker compose run dbmate new --",
    "apply-migrations": "docker compose run order-dbmate"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "tsoa",
    "hexagonal-architecture",
    "rabbitmq"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@alejotamayo28/event-contracts": "^1.0.2",
    "@tsoa/runtime": "^5.0.0",
    "amqplib": "^0.10.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "moment": "^2.30.1",
    "pg": "^8.16.3",
    "pg-format": "^1.0.4",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.9",
    "@types/amqplib": "^0.10.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@types/pg": "^8.16.0",
    "@types/pg-format": "^1.0.5",
    "@types/redis": "^4.0.10",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.0.3",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsoa": "^5.1.1",
    "typescript": "^5.3.3"
  }
}
